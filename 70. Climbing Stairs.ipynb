   "source": [
    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        ls = [1,2]\n",
    "        if n < 3:\n",
    "            return ls[n - 1]\n",
    "        while len(ls) != n:\n",
    "            ls.append(ls[-1] + ls[-2])\n",
    "        return ls[-1]"
   ]
  }
