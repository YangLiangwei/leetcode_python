    "class Solution:\n",
    "    def isPowerOfTwo(self, n: int) -> bool:\n",
    "        if n == 0:\n",
    "            return False\n",
    "        while n != 1:\n",
    "            if n // 2 != n / 2:\n",
    "                return False\n",
    "            n = n / 2\n",
    "        return True"
   ]
  }
