   "source": [
    "class Solution:\n",
    "    def trailingZeroes(self, n: int) -> int:\n",
    "        if n == 0:\n",
    "            return 0\n",
    "        num = 0\n",
    "        while n > 1:\n",
    "            num += int(n / 5)\n",
    "            n = n / 5\n",
    "        return int(num)"
   ]
  }
